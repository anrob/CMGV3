<% @title = "Showing actcode" %>

<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
      <div class="form-horizontal">
                <div class="form-group">
          <label class="col-md-2 form-label">Actcode</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @actcode.actcode %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">Management</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @actcode.management_id %>
            </span>
          </div>
        </div>

                <div class="form-group">
          <label class="col-md-2 form-label">Description</label>
          <div class="col-md-10 form-control uneditable-input">
            <span>
              <%= @actcode.description %>
            </span>
          </div>
        </div>



                <div class="form-actions col-md-offset-2">
          <%= link_to fa_icon_tag("pencil") + ' Edit', edit_actcode_path(@actcode), class: 'btn btn-primary' %>
          <%= link_to 'Back', actcodes_path, class: 'btn' %>
        </div>
      </div>
    </div>
  </div>
</div>
