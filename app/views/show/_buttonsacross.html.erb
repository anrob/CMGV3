<div class="row text-center blue-grey-900">
<div class="col-xs-12 col-sm-12 col-md-2 col-md-offset-1 col-lg-2 grey-50">
    <h5><%= contract.act_booked %></h5>
 
<div class="small"></div>
</div>
<div class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
    <% if contract.unconfirmed? && contract.date_of_event < Date.today + 11.days  %>
<h4 class="text-indigo-50"> <%=   link_to ' CONFIRM THIS EVENT ', confirmjob_contract_path(contract.id), class: 'btn btn-flat btn-danger' %></h4>
<% end %>
<span class="metric-value">

</span>
<div class="small"></div>
</div>


 <% unless contract.unconfirmed? %>
<div class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
<h4><% if current_user.try(:type) == "Super" %><%= link_to 'Email All Money', emailjobwithallmoney_path(id: contract.id), class: 'btn btn-flat btn-sm btn-danger' %><% end %> </h4>
<span class="metric-value">

</span>
<div class="small"></div>
</div>
<div class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
<h4><% if current_user.try(:type) == "Super" %><%= link_to 'Email Net Money', emailjobwithnetonly_path(id: contract.id), class: 'btn btn-flat btn-sm btn-success' %> <% end %></h4>
<span class="metric-value">

</span>
<div class="small"></div>
</div>
<div class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
<h4><%= link_to 'Email Job Info', emailjobnomoney_path(id: contract.id), class: 'btn btn-flat btn-sm btn-warning' %></h4>
<span class="metric-value">

</span>
<div class="small"></div>
</div>
<% end %>
</div>

