<%= form_for(@job, html: { class: 'form-horizontal' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.form_group :user_id do |f| %>
      <%= f.label :user_id, class: 'control-label col-md-2' %>
      <div class='col-md-8'>
        <%= f.number_field :user_id, class: 'form-control' %>
        <%= f.error_messages %>
      </div>
    <% end %>

    <%= f.form_group :contract_id do |f| %>
      <%= f.label :contract_id, class: 'control-label col-md-2' %>
      <div class='col-md-8'>
        <%= f.number_field :contract_id, class: 'form-control' %>
        <%= f.error_messages %>
      </div>
    <% end %>

   

  </div>

  <div class="form-actions col-md-offset-2 col-md-10">
    <%= f.submit class: 'btn btn-primary' %>
    <%= link_to "Cancel", jobs_path, class: 'btn' %>
  </div>
<% end %>